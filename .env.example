# Required
GEMINI_API_KEY=your_gemini_api_key

# Ad slot render mode: development | none | vercel
# development: render gray placeholders for local debugging
# none: render nothing (recommended during AdSense review)
# vercel: render real AdSense slots and load AdSense/Adsterra scripts
VITE_AD_PLACEHOLDER_MODE=development

# AdSense (required for vercel mode)
VITE_ADSENSE_CLIENT_ID=ca-pub-xxxxxxxxxxxxxxxx
VITE_ADSENSE_SLOT_BANNER=1234567890
VITE_ADSENSE_SLOT_SIDEBAR=1234567890
VITE_ADSENSE_SLOT_RECT=1234567890

# Adsterra (optional, loaded only in vercel mode)
VITE_ADSTERRA_SCRIPT_URL=
